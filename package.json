{
  "name": "@guestbell/pdf-template",
  "version": "0.0.1",
  "description": "Create pdf based templates on the frontend using react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "api-docs": "typedoc --plugin typedoc-plugin-markdown --out api-docs src/index.ts",
    "build": "rollup -c",
    "version:patch": "npm version patch",
    "push:git": "git push --tags origin main --quiet",
    "commit:build": "git add . && git commit -m \"Deploy commit\" --quiet && npm run push:git || true",
    "create:patch": "npm run version:patch && npm run push:git",
    "deploy": "npm run api-docs && npm run build && npm run build-storybook && npm run commit:build && npm publish && npm run create:patch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestbell/pdf-template.git"
  },
  "keywords": [
    "react",
    "onboarding",
    "wizard"
  ],
  "author": "Peter Kottas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guestbell/pdf-template/issues"
  },
  "homepage": "https://guestbell.github.io/pdf-template/",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "7.0.2",
    "@storybook/addon-interactions": "7.0.2",
    "@storybook/addon-links": "7.0.2",
    "@storybook/blocks": "7.0.2",
    "@storybook/react": "7.0.2",
    "@storybook/react-webpack5": "7.0.2",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "dayjs": "^1.11.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prop-types": "15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.11.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^4.0.0",
    "storybook": "7.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
